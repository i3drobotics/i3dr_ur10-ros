<?xml version="1.0"?>
<!--XML-->
<launch>
    <!-- SETUP ARGUMENTS -->
    <!-- ur10 arguments -->
    <arg name="robot_ip" default="172.16.1.58"/>

    <include file="$(find ur_bringup)/launch/ur10_bringup.launch">
        <arg name="robot_ip" value="$(arg robot_ip)"/>
    </include>

    <include file="$(find ur10_moveit_config)/launch/ur10_moveit_planning_execution.launch">
    </include>

    <include file="$(find ur10_moveit_config)/launch/moveit_rviz.launch">
        <arg name="config" value="true"/>
    </include>

</launch>